{
  "expo": {
    "name": "bri-SPY",
    "slug": "bri-spy",
    "privacy": "public",
    "sdkVersion": "35.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#FC354C"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/foreground.png",
        "backgroundColor": "#FC354C",
        "backgroundImage": "./assets/background.png"
      }
    },
    "description": "bri-SPY is a game in which users pinch-to-zoom and pan to navigate around an image in order to find a small hidden icon. There is a growing library of photos to play through."
  }
}
