      "bundleIdentifier": "com.bri-spy.bri-spy",
      "buildNumber": "1.0.0",
      "supportsTablet": true, 
      "icon": "./assets/iconAppleNoText.png"
    },
    "android": {
      "package": "com.bri_spy.bri_spy",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/foregroundIcon.png",
        "backgroundColor": "#FC354C",
        "backgroundImage": "./assets/background.png"
      },
      "permissions": []
    },
    "description": "bri-SPY is a game in which users pinch-to-zoom and pan to navigate around an image in order to find a small hidden icon. There is a growing library of photos to play through."
  }
}
