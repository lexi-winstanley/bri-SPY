{
  "expo": {
    "name": "bri-SPY",
    "slug": "bri-spy",
    "privacy": "unlisted",
    "sdkVersion": "35.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "primaryColor": "#FC354C",
    "icon": "./assets/iconAppleNoText.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#FC354C"
    },
    "updates": {
